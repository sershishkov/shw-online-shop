<header>
  <div>
    <a href="/">WDE</a>
  </div>
  <nav>
    <ul>
      <% if (locals.isAuth) { %>       
        <li>
          <form action="/logout" method="POST">
            <input type="hidden" name="_csrf" value="<%= locals.csrfToken %>" >
            <button >Logout</button>
          </form>
        </li>
      <% } else { %>
        <li>
          <a href="/login">Login</a>
        </li>
        <li>
          <a href="/signup">Signup</a>
        </li>
        <% }  %>  
        
    </ul>
  </nav>
</header>
